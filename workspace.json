{
  "version": 2,
  "projects": {
    "client-server": {
      "root": "apps/client-server",
      "sourceRoot": "apps/client-server/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/client-server",
            "main": "apps/client-server/src/main.ts",
            "tsConfig": "apps/client-server/tsconfig.app.json",
            "assets": ["apps/client-server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/client-server/src/environments/environment.ts",
                  "with": "apps/client-server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "client-server:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/client-server/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/client-server"],
          "options": {
            "jestConfig": "apps/client-server/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "fs": {
      "root": "libs/fs",
      "sourceRoot": "libs/fs/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/fs/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/fs"],
          "options": {
            "jestConfig": "libs/fs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "http": {
      "root": "libs/http",
      "sourceRoot": "libs/http/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/http/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/http"],
          "options": {
            "jestConfig": "libs/http/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "postybirb": {
      "root": "apps/postybirb",
      "sourceRoot": "apps/postybirb/src",
      "projectType": "application",
      "prefix": "postybirb",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "options": {
            "outputPath": "dist/apps/postybirb",
            "main": "apps/postybirb/src/main.ts",
            "tsConfig": "apps/postybirb/tsconfig.app.json",
            "assets": ["apps/postybirb/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/postybirb/src/environments/environment.ts",
                  "with": "apps/postybirb/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "postybirb:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "postybirb",
            "frontendProject": "ui",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "postybirb",
            "frontendProject": "ui",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/postybirb/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/postybirb"],
          "options": {
            "jestConfig": "apps/postybirb/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui": {
      "root": "apps/ui",
      "sourceRoot": "apps/ui/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ui",
            "index": "apps/ui/src/index.html",
            "main": "apps/ui/src/main.tsx",
            "polyfills": "apps/ui/src/polyfills.ts",
            "tsConfig": "apps/ui/tsconfig.app.json",
            "assets": ["apps/ui/src/favicon.ico", "apps/ui/src/assets"],
            "styles": ["apps/ui/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ui/src/environments/environment.ts",
                  "with": "apps/ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "ui:build",
            "hmr": true,
            "proxyConfig": "apps/ui/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "ui:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ui"],
          "options": {
            "jestConfig": "apps/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-e2e": {
      "root": "apps/ui-e2e",
      "sourceRoot": "apps/ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-e2e/cypress.json",
            "tsConfig": "apps/ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ui:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "ui"
}
